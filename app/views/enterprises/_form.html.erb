<%= simple_form_for(@enterprise) do |f| %>
  <div class="form-group">

    <%= f.input :name, class: 'form-control' %>
  </div>

  <div class="form-group">

    <%= f.input :abstract, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
  </div>

  <%= f.submit class: 'btn btn-primary' %>
<% end %>
