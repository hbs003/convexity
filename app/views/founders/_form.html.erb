<%= simple_form_for [@enterprise, @founder] do |f| %>
  <%= f.input :name, :label => 'Nome:', :required =>false %>
  <%= f.input :function, :label => 'Função na empresa:', :required =>false %>
  <%= f.input :description, :label => 'Descrição resumida:', :required =>false %>
  <%= f.submit "Criar novo fundador", class: "btn btn-primary" %>
<% end %>
